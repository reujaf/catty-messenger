      },
      "package": "com.catty.messenger",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2b61fd75-5e64-4ae4-8bb7-7e4b8a83c251"
      }
